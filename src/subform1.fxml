<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="425.0" prefWidth="875.0" stylesheets="@main1.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.echomap.kqf.two.gui.KQFSubCtrl">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints vgrow="SOMETIMES" />
		<RowConstraints fillHeight="false" vgrow="SOMETIMES" />
		<RowConstraints vgrow="SOMETIMES" />
		<RowConstraints minHeight="200.0" percentHeight="50.0" vgrow="ALWAYS" />
	</rowConstraints>
	<children>
		<HBox>
			<children>
				<HBox style="-fx-border-color: black;">
					<children>
						<Label minWidth="50.0" text="Name:">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Label>
						<TextField fx:id="inputName" />
					</children>
               <padding>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </padding>
				</HBox>
				<HBox style="-fx-border-color: black;" HBox.hgrow="ALWAYS">
					<children>
						<Label minWidth="50.0" text="File:">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Label>
						<TextField fx:id="inputFile" HBox.hgrow="ALWAYS" />
						<Button minWidth="75.0" mnemonicParsing="false" onAction="#handleBrowse" styleClass="actionbutton" text="Browse">
                     <HBox.margin>
                        <Insets bottom="6.0" left="4.0" />
                     </HBox.margin></Button>
					</children>
               <padding>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </padding>
				</HBox>
			</children>
		</HBox>
		<VBox spacing="10.0" style="-fx-border-color: black;" GridPane.rowIndex="1">
			<children>
				<Label text="DocTags">
					<HBox.margin>
						<Insets />
					</HBox.margin>
				</Label>
				<TextArea fx:id="inputDocTags" minHeight="150.0" />
			</children>
		</VBox>
		<HBox alignment="CENTER" spacing="20.0" style="-fx-background-color: gray;" GridPane.rowIndex="2">
			<children>
            <Button onAction="#handleNew" styleClass="newbutton" text="_New">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
				<Button fx:id="buttonAddEdit" onAction="#handleAddEdit" styleClass="actionbutton" text="_Add/Edit">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
            <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#handleDelete" styleClass="deletebutton" text="Delete">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
            <Separator orientation="VERTICAL" />
            <Button fx:id="buttonExport" onAction="#handleExport" text="_Export">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
            <Button fx:id="buttonImport" onAction="#handleImport" text="_Import">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
            <Separator orientation="VERTICAL" />
            <Button fx:id="buttonSave" onAction="#handleSave" styleClass="savebutton" text="_Save Data">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
				<Button fx:id="buttonClose" cancelButton="true" defaultButton="true" onAction="#handleClose" style="-fx-background-color: #6473A4;" styleClass="actionbutton" text="_Close">
               <HBox.margin>
                  <Insets bottom="6.0" />
               </HBox.margin></Button>
			</children>
		</HBox>
		<ScrollPane fitToHeight="true" fitToWidth="true" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
         <content>
                           <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
               <children>
            				<TableView fx:id="inputTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
  
                  
               </children>
            </GridPane>
         </content>
		</ScrollPane>
	</children>
</GridPane>
